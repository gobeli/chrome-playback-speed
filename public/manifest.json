{
  "name": "playbackspeed",
  "version": "1.0",
  "description": "Set playbackspeed on <video>'s",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.svg",
      "32": "icon.svg",
      "48": "icon.svg",
      "128": "icon.svg"
    }
  },
  "content_scripts": [
      {
      "matches": [
          "http://*/*",
          "https://*/*"
          ],
      "js": ["content.js"],
      "run_at": "document_end"
      }
  ],
  "permissions": ["tabs", "activeTab", "declarativeContent"],
  "manifest_version": 2
}